package computerscience.java.examples;

class PrimitiveInt2 {
public static void main(String[] args) {
int i; int j
//create a variable called i,j
i = 1; j=5;
//update i with the value 1, j with the value 5
i = j
//update i with the value of j
System.out.println(i); //print out the current value of i
}
}

