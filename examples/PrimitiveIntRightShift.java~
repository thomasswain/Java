package computerscience.java.examples;

class PrimitiveIntRightShift {
public static void main(String[] args) {
int i;
//create a variable called i
i = 0x055>>1;
//update i with the value 55 in hexadecimal and use right shift 1 to get result 5.

//update i with the value of "i+1"
System.out.println(i); //print out the current value of i
}
}

